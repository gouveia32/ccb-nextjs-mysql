{
  "name": "notes-app",
  "version": "1.0.0",
  "description": "Notes app implementation inspired by Google Keep developed with NextJS and Prisma.",
  "author": "Jan Bauer",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "test": "jest",
    "export": "export"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.4",
    "@next-auth/prisma-adapter": "^0.5.3",
    "@prisma/client": "^2.28.0",
    "@reduxjs/toolkit": "^1.6.1",
    "@svgr/webpack": "^5.5.0",
    "@types/material-ui": "^0.21.9",
    "@types/react-redux": "^7.1.18",
    "@types/styled-components": "^5.1.11",
    "babel-plugin-styled-components": "^1.13.2",
    "bootstrap": "^5.0.0-beta3",
    "framer-motion": "^4.1.17",
    "next": "11.0.1",
    "next-auth": "^3.27.3",
    "react": "17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "react-redux": "^7.2.4",
    "react-toastify": "^7.0.4",
    "redux-saga": "^1.1.3",
    "sass": "^1.36.0",
    "sqlite3": "^5.0.2",
    "styled-components": "^5.3.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.21",
    "@types/node": "^16.4.3",
    "@types/react": "17.0.14",
    "@types/react-color": "^3.0.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.5.0",
    "autoprefixer": "^10.3.1",
    "enzyme": "^3.11.0",
    "eslint": "7.31.0",
    "eslint-config-next": "11.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.3.6",
    "prisma": "^2.27.0",
    "ts-jest": "^26.5.4",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|scss|sass|less)$": "<rootDir>/style-mock.js",
      ".+\\.(svg|png|jpg)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.jest.json"
      }
    }
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
